bazel_dep(name = "gazelle", version = "0.35.0")
bazel_dep(name = "rules_go", version = "0.44.1" )

bazel_dep(name = "org_golang_x_example_hello")

local_path_override(
    module_name = "org_golang_x_example_hello",
    path = "./example/hello",
)

bazel_dep(name = "hello")
local_path_override(
    module_name = "hello",
    path = "./hello",
)

go_sdk = use_extension("@rules_go//go:extensions.bzl", "go_sdk")
go_sdk.download(version = "1.20.3")
